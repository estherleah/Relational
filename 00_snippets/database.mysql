# MySQL code snippets

####################
# Create Statement #
####################

CREATE TABLE `User` (
  `userID` INT NOT NULL AUTO_INCREMENT,
  `email` TEXT,
  `password` TEXT,
  `firstName` TEXT,
  `lastName` TEXT,
  `dob` DATE,
  `gender` CHAR,
  `location` TEXT
);

CREATE TABLE `Friendship` (
  `userID1` INT,
  `userID2` INT
);

CREATE TABLE `PhotoCollection` (
  `collectionID` INT NOT NULL AUTO_INCREMENT,
  `userID` INT,
  `privacyID` INT
);

CREATE TABLE `Circle` (
  `circleID` INT NOT NULL AUTO_INCREMENT,
  `name` TEXT,
  `description` TEXT
);

CREATE TABLE `CircleParticipants` (
  `circleID` INT,
  `userID` INT
);

CREATE TABLE `PrivacySettings` (
  `privacyID` INT NOT NULL AUTO_INCREMENT,
  `option` INT
);

CREATE TABLE `PhotoComment` (
  `commentID` INT NOT NULL AUTO_INCREMENT,
  `photoID` INT,
  `userID` INT,
  `comment` TEXT
);

CREATE TABLE `Photo` (
  `photoID` INT NOT NULL AUTO_INCREMENT,
  `collectionID` INT,
  `photoURL` TEXT
);

CREATE TABLE `Message` (
  `messageID` INT NOT NULL AUTO_INCREMENT,
  `circleID` INT,
  `userID` INT,
  `message` TEXT
);

CREATE TABLE `PhotoAnnotation` (
  `annotationID` INT NOT NULL AUTO_INCREMENT,
  `photoID` INT,
  `userID` INT
);

CREATE TABLE `BlogEntry` (
  `entryID` INT ,
  `userID` INT,
  `entry` TEXT,
  `privacyID` INT
);
